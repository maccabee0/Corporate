<Window x:Class="Corporate.WPF.Shell"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://www.codeplex.com/prism"
        xmlns:regions="clr-namespace:Corporate.Domain;assembly=Corporate.Domain"
        Title="Expendeture Reporting" Height="526" Width="768" >
    <Grid Background="{StaticResource MainBackground}" >
        <Label HorizontalAlignment="Right" VerticalAlignment="Top" Margin="10,10" Foreground="{StaticResource TextBrush}" FontSize="50">Corporation</Label>
        <ContentControl prism:RegionManager.RegionName="{x:Static regions:RegionNames.MainRegion}">
    </ContentControl>
    </Grid>
</Window>
